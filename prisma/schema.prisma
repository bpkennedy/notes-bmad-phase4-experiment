generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Note {
  id        String   @id @default(uuid())
  title     String
  content   String   @db.LongText
  createdAt DateTime @default(now()) @db.DateTime(3)

  @@index([createdAt])
}
